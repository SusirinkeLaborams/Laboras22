<Window x:Class="Laboras22.Views.ProjectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ProjectWindow" Height="300" Width="300">
    <StackPanel>
        <TextBlock x:Name="AssignmentName" Text="Assignment Name"/>
        <Button x:Name="AddContentButton" Content="Add Content"/>
        <Button x:Name="AddParticipantsButton" Content="Add Participants"/>
        <Button x:Name="SubmitButton" Content="Submit For Review" />
        <Button x:Name="DeleteProject" Content="Delete Project"/>
    </StackPanel>
</Window>
