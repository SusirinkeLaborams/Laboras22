<Laboras22:PageBase x:Class="Laboras22.Views.ProjectPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Laboras22="clr-namespace:Laboras22.Views.Pages"
        Title="ProjectWindow" Height="300" Width="300">
    <StackPanel x:Name="root">
        <TextBlock Text="{Binding AssignmentName}"/>
        <TextBlock Text="{Binding Name}"/>
        <ListView ItemsSource="{Binding Participants}">
            <TextBlock Text="{Binding Name}"/>
        </ListView>
    </StackPanel>
</Laboras22:PageBase>
